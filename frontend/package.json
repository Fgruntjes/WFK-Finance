{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run build:api && npm run build:code",
		"build:code": "vite build",
		"build:api": "rm -Rvf src/api/generated && openapi-generator-cli generate -g typescript-fetch -i http://localhost:5000/swagger/v1/swagger.json -o src/api/generated --additional-properties=supportsES6=true,npmVersion=9.6.5",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@auth0/auth0-spa-js": "^2.0.5",
		"@lukemorales/query-key-factory": "^1.2.0",
		"@openapitools/openapi-generator-cli": "^2.6.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@tanstack/svelte-query": "^4.29.7",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"carbon-components-svelte": "^0.75.0",
		"carbon-icons-svelte": "^11.4.0",
		"carbon-preprocess-svelte": "^0.9.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"i18next": "^22.5.0",
		"i18next-http-backend": "^2.2.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-i18next": "^2.0.0",
		"svelte-local-storage-store": "^0.4.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"remove": "^0.1.5"
	}
}
